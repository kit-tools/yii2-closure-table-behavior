{
    "name": "kittools/yii2-closure-table-behavior",
    "description": "Closure Table behavior.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Zimoglyadov",
            "email": "wppdevelop@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "^2.0.15"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.1",
        "codeception/module-db": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "kittools\\closuretable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
